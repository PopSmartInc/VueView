{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.formatOnSave": true,
  "vetur.format.scriptInitialIndent": true,
  "vetur.format.styleInitialIndent": true,
  "prettier.useTabs": true,
  "prettier.enable": false,
  "prettier.semi": false,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "vue",
    "html"
  ],
}